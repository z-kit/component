{
  "name": "z-component",
  "version": "1.0.0",
  "description": "z-kit component template",
  "main": "dist/index.js",
  "minified:main": "dist/index.min.js",
  "jsnext:main": "dist/module.js",
  "module": "dist/module.js",
  "scripts": {
    "lint": "eslint webpack.config.babel.js .storybook src stories test && stylelint src/style.css",
    "test": "echo \"Lets figure the tests out!\"",
    "build-module": "mkdirp dist && babel --source-maps -o dist/module.js src/component.js",
    "package": "webpack",
    "min-package": "MIN=true webpack -p",
    "build": "npm run build-module && npm run package && npm run min-package",
    "start": "npm run storybook",
    "clean": "rimraf dist",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/z-kit/component.git"
  },
  "keywords": [
    "z-kit",
    "component"
  ],
  "author": "Matheus Kautzmann",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/z-kit/component/issues"
  },
  "homepage": "https://github.com/z-kit/component",
  "devDependencies": {
    "@kadira/storybook": "^2.21.0",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-loader": "^6.2.8",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "css-loader": "^0.26.0",
    "eslint": "^3.8.0",
    "eslint-config-airbnb": "^12.0.0",
    "eslint-import-resolver-webpack": "^0.7.0",
    "eslint-plugin-import": "^1.16.0",
    "eslint-plugin-jsx-a11y": "^2.2.2",
    "eslint-plugin-react": "^6.3.0",
    "extract-text-webpack-plugin": "2.0.0-beta.4",
    "file-loader": "^0.9.0",
    "handlebars": "^4.0.6",
    "handlebars-loader": "^1.4.0",
    "html-webpack-plugin": "^2.24.1",
    "mkdirp": "^0.5.1",
    "ncp": "^2.0.0",
    "postcss-loader": "^1.2.0",
    "react": "^15.4.1",
    "react-dom": "^15.4.1",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "stylelint": "^7.6.0",
    "stylelint-config-standard": "^15.0.0",
    "stylelint-selector-bem-pattern": "^1.0.0",
    "webpack": "v2.1.0-beta.27"
  }
}
